{
  "name": "murmurations-aggregator-map",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPLv3",
  "scripts": {
    "build": "npm-run-all --sequential build:*",
    "build:prod": "webpack --mode production",
    "build:css": "tailwindcss -i src/styles/styles.css -o ../assets/map/index.css --minify",
    "dev": "npm-run-all --parallel watch:*",
    "watch:dev": "webpack --watch",
    "watch:css": "tailwindcss -i ./src/styles/styles.css -o ../assets/map/index.css --watch --minify",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
    "lint-fix": "eslint --ignore-path .gitignore --ext .js --ext .jsx . --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "prepare": "cd ../.. && husky install admin/map/.husky"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-prettier"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run lint-fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@changey/react-leaflet-markercluster": "^4.0.0-rc1",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "esbuild-plugin-eslint": "^0.3.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
