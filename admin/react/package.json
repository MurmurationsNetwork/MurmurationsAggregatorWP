{
  "name": "murmurations-aggregator-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPLv3",
  "scripts": {
    "build": "npm-run-all --sequential build:*",
    "build:prod": "webpack --mode production",
    "build:css": "tailwindcss -i src/styles/styles.css -o ../assets/react/index.css --minify",
    "dev": "npm-run-all --parallel watch:*",
    "watch:dev": "webpack --watch",
    "watch:css": "tailwindcss -i ./src/styles/styles.css -o ../assets/react/index.css --watch --minify",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
    "lint-fix": "eslint --ignore-path .gitignore --ext .js --ext .jsx . --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "prepare": "cd ../.. && husky install admin/react/.husky",
    "readme": "Grunt"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-prettier"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run lint-fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-diff-viewer-continued": "^3.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "esbuild-plugin-eslint": "^0.3.6",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "grunt": "^1.6.1",
    "grunt-wp-readme-to-markdown": "^2.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
